<%- include layouts/header.ejs %>

        <div class="page-data">
            <form method="post" action="/schedules/edit/<%=schedule._id%>">
                <table cellpadding="11">
                <tr>
                    <td class="label">Schedule Name</td><td>: <input type="text" name="schedule_name" value="<%=schedule.schedule_name%>" required></td>
                 </tr>
                 <tr>
                    <td class="label" valign="top">Address</td>
                    <td valign="top">: 
                        <textarea name="address" required cols="30" rows="5"><%=schedule.address%></textarea>
                    </td>
                </tr>
                <tr>
                    <td class="label">Company Name</td><td>: <input type="text" value="<%=schedule.company_name%>" name="company_name" required></td>
                 </tr>
                  <tr>
                    <td class="label">Candidate Name</td><td>: <input type="text" value="<%=schedule.candidate_name%>"  name="candidate_name" required></td>
                 </tr>
				 <tr>
                    <td class="label">Contact Name</td><td>: <input type="text" value="<%=schedule.contact_person%>"  name="contact_person" required></td>
                 </tr>
                 <tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
            </form>
            
        </div>
        
<%- include layouts/footer.ejs %>
